//Is composite number
//Простое или составное число
#include <iostream>

using namespace std;
int main() {
  cout << "Input Number: ";
  long int a;
  cin >> a;
  if (a != 2 && a != 3 && a != 0 && a != 1) {
    if (a % 2 == 0 || a % 3 == 0) {
      cout << "Number " << a << " is composite";
    }

  } else {
    cout << "Number " << a << " isn't composite";
  }

}
